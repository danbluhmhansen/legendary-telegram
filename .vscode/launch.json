{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Server",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "buildServer",
			"program": "${workspaceFolder}/Source/Server/bin/Debug/net6.0/BlazorApp1.Server.dll",
			"args": [],
			"cwd": "${workspaceFolder}/Source/Server",
			"stopAtEntry": false,
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"sourceFileMap": {
				"/Views": "${workspaceFolder}/Source/Server/Views"
			}
		},
		{
			"name": "Launch Client",
			"type": "blazorwasm",
			"request": "launch",
			"preLaunchTask": "buildClient",
			"cwd": "${workspaceFolder}/Source/Client"
		},
		{
			"name": "Launch Test Console",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "buildConsole",
			"program": "${workspaceFolder}/Tests/Console/bin/Debug/net6.0/BlazorApp1.Console.dll",
			"args": [],
			"cwd": "${workspaceFolder}/Tests/Console",
			"stopAtEntry": false,
			"console": "internalConsole"
		}
	]
}
